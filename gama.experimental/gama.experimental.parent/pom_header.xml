<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.gama</groupId>
	<artifactId>gama.experimental.parent</artifactId>
	<version>2024.6.0-SNAPSHOT</version>
	<packaging>pom</packaging>
	
	<properties>
		<tycho.version>4.0.8</tycho.version>
		<jdk.version>21</jdk.version>
		<gama.version>[2024.6.0,)</gama.version>
		<gama.p2.version>2024-06</gama.p2.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<wagon.version>3.5.3</wagon.version>
	</properties>

	<repositories>
		<repository>
			<id>p2Repo</id>
			<name>p2Repo</name>
			<url>https://updates.gama-platform.org/${gama.p2.version}</url>
			<layout>p2</layout>
			<snapshots>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</snapshots>
			<releases>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</releases>
		</repository>
		<repository>
			<id>gamaRepo</id>
			<name>gamaRepo</name>
			<url>https://updates.gama-platform.org/${gama.p2.version}</url>
			<layout>default</layout>
			<snapshots>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</snapshots>
			<releases>
				<enabled>true</enabled>
				<checksumPolicy>ignore</checksumPolicy>
			</releases>
		</repository>
		<repository>
			<id>eclipse-swt</id>
			<url>https://download.eclipse.org/releases/2024-06</url>
			<layout>p2</layout>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>

